    <license>
      <name>Copyright (c) 2018 SPP. All Rights Reserved.</name>
      <url></url>
    </license>
  </licenses>
  <dependencies>
    <dependency>
      <groupId>org.springframework.boot</groupId>
      <artifactId>spring-boot-starter-web</artifactId>
      <version>1.5.13.RELEASE</version>
